{
    "name": "gitbook-shelf",
    "displayName": "Gitbook shelf",
    "description": "Extension to makes VSCode Markdown Editor for Gitbook",
    "version": "0.1.0",
    "publisher": "EnskyLin",
    "engines": {
        "vscode": "^1.17.0"
    },
    "categories": [
        "Programming Languages",
        "Other"
    ],
    "activationEvents": [
        "workspaceContains:./SUMMARY.md"
    ],
    "main": "./dist/index",
    "contributes": {
        "views": {
            "explorer": [
                {
                    "id": "bookstruct",
                    "name": "GitBook",
                    "when": "showOutline"
                }
            ]
        },
        "commands": [
            {
                "command": "gitbook.openDoc",
                "title": "Edit"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "webpack --mode production",
        "webpack": "webpack --mode development",
        "webpack-dev": "webpack --mode development --watch",
        "package": "vsce package",
        "publish": "vsce publish"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.42",
        "@types/node": "^7.0.0",
        "mocha": "^3.5.0",
        "standard": "^14.3.1",
        "typescript": "^2.5.2",
        "vscode": "^1.1.5",
        "webpack": "^4.41.2",
        "webpack-cli": "^3.3.10"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/ensky/vscode-gitbook-shelf"
    },
    "homepage": "https://github.com/ensky/vscode-gitbook-shelf",
    "dependencies": {
        "@gitbook/slate": "^4.0.4",
        "immutable": "^4.0.0-rc.12",
        "markup-it": "^12.0.1",
        "npm": "^6.13.4",
        "react": "^16.12.0",
        "vsce": "^1.71.0"
    }
}
